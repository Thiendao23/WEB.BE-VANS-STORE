<div class="tieu-de">
    <div class="dong-tieu-de">
        <h1>Đăng kí tài khoản </h1>
    </div>
    <div class="duong-dan">
        <span><a href="@Url.Action("Index", "TrangChu")">Trang chủ</a></span>
        <span>/</span>
        <span class="chu-do">Đăng kí tài khoản</span>
    </div>
</div>
<div class="mid container">
    
    <div class="middle-content">
        <div class="head-content">
            <h1>Đăng ký</h1>
        </div>
        <div id="error"></div>
        <form id="form" action="/" method="get">
            <div class="body-content">
                <div class="groub">

                    <label for="ho">Họ<span class="sym">*</span></label>
                    <input id="ho" name="ho" type="text" placeholder="Họ" class="style-text" required>

                    <label for="name">Tên<span class="sym">*</span></label>
                    <input id="name" name="name" type="text" placeholder="Tên" class="style-text" required>

                    <label for="email">Email<span class="sym">*</span></label>
                    <input id="email" name="email " type="text" placeholder="Email" class="style-text" required>

                    <label for="number">Số điện thoại<span class="sym">*</span></label>
                    <input id="number" name="numer " type="text" placeholder="Số điện thoại" class="style-text" required>

                    <label for="password">Mật khẩu<span class="sym">*</span></label>
                    <input id="password" name="password" type="password" placeholder="Mật khẩu" class="style-text" required>

                    <div class="submit" type="submit"><a href="@Url.Action("Index", "TrangChu")">Đăng ký</a></div>

                </div>
            </div>
        </form>

    </div>
